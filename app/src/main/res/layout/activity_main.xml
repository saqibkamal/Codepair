<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="kamal.saqib.codepair.MainActivity">

    <ImageView
        android:layout_width="400dp"
        android:layout_height="100dp"
        android:src="@drawable/codechef"
        android:id="@+id/codechef"
        android:layout_marginTop="168dp"
        android:layout_marginLeft="6dp"
        android:layout_marginStart="6dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/spoj"
        android:layout_alignStart="@+id/spoj" />

    <ImageView
        android:layout_width="250dp"
        android:layout_height="100dp"
        android:src="@drawable/spoj"
        android:id="@+id/spoj"
        android:layout_marginTop="39dp"
        android:layout_below="@+id/codechef"
        android:layout_centerHorizontal="true" />



</RelativeLayout>
